{
  "name": "@hideoo/prettier-config",
  "version": "2.0.0",
  "license": "MIT",
  "description": "HiDeoo's Prettier configuration",
  "author": "HiDeoo <github@hideoo.dev> (https://hideoo.dev)",
  "main": ".prettierrc.json",
  "scripts": {
    "lint": "prettier -c --cache .",
    "prepare": "husky install"
  },
  "devDependencies": {
    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "prettier": "3.0.0"
  },
  "packageManager": "pnpm@7.23.0",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    ".prettierrc.json"
  ],
  "keywords": [
    "prettier",
    "config",
    "prettier-config"
  ],
  "homepage": "https://github.com/HiDeoo/prettier-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/HiDeoo/prettier-config.git"
  },
  "bugs": "https://github.com/HiDeoo/prettier-config/issues",
  "lint-staged": {
    "**/*": "prettier -w -u --cache"
  }
}
